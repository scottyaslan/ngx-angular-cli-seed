  "name": "ngx-angular-cli-seed",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --codeCoverage=true",
    "test:watch": "ng test --watch=true --karmaConfig=karma.watch.conf.js",
    "lint": "npm run lint:styles && npm run lint:eslint",
    "lint:eslint": "eslint --ext .ts webapp/",
    "lint:styles": "stylelint webapp/**/*.scss webapp/**/*.css !webapp/platform/assets/**",
    "lint:fix": "eslint --fix --ext .ts webapp/",
    "lint:staged": "lint-staged",
    "check:full": "npm run lint && ng build --aot=true && npm test",
    "xi18n": "ng xi18n --output-path webapp/locale",
    "preinstall": "npx npm-force-resolutions"
  },
  "private": true,
  "description": "Angular CLI seed",
  "keywords": [
    "angular",
    "material",
    "material design",
    "components",
    "reusable"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "9.1.6",
    "@angular/cdk": "9.2.3",
    "@angular/common": "9.1.6",
    "@angular/compiler": "9.1.6",
    "@angular/core": "9.1.6",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.1.6",
    "@angular/localize": "9.1.6",
    "@angular/material": "9.2.3",
    "@angular/platform-browser": "9.1.6",
    "@angular/platform-browser-dynamic": "9.1.6",
    "@angular/platform-server": "9.1.6",
    "@angular/router": "9.1.6",
    "detect-libc": "1.0.3",
    "hammerjs": "2.0.8",
    "lodash-core": "4.17.11",
    "reset-css": "4.0.1",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.5",
    "tslib": "1.11.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.5",
    "@angular/cli": "9.1.4",
    "@angular/compiler-cli": "9.1.6",
    "@angular/language-service": "9.1.6",
    "@ngneat/spectator": "5.6.1",
    "@types/d3": "4.13.0",
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jasmine": "4.1.0",
    "husky": "3.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-parallel": "0.3.1",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "9.5.0",
    "ng-lint-staged": "0.1.6",
    "ng-mocks": "8.1.0",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "19.0.0",
    "ts-node": "7.0.0",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint",
    "*.css": "stylelint",
    "*.scss": "stylelint --syntax=scss"
  },
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "resolutions": {
    "yargs-parser": "18.1.3"
  }
}
