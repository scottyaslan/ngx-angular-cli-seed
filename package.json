    "name": "ngx-angular-cli-seed",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:prod": "ng serve --prod",
        "build:dev": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test --codeCoverage=true",
        "test:watch": "ng test --watch=true --karmaConfig=karma.watch.conf.js",
        "lint": "npm run lint:styles && npm run lint:eslint",
        "lint:eslint": "eslint --ext .ts webapp/",
        "lint:styles": "stylelint webapp/**/*.scss webapp/**/*.css !webapp/platform/assets/**",
        "lint:fix": "eslint --fix --ext .ts webapp/",
        "lint:staged": "lint-staged",
        "check:full": "npm run lint && ng build --aot=true && npm test",
        "xi18n": "ng xi18n --output-path webapp/locale",
        "preinstall": "npx npm-force-resolutions"
    },
    "private": true,
    "description": "Angular CLI seed",
    "keywords": [
        "angular",
        "material",
        "material design",
        "components",
        "reusable"
    ],
    "license": "Apache-2.0",
    "dependencies": {
        "@angular/animations": "8.1.3",
        "@angular/cdk": "8.1.2",
        "@angular/common": "8.1.3",
        "@angular/compiler": "8.1.3",
