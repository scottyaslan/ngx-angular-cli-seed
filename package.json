    "name": "ngx-angular-cli-seed",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:prod": "ng serve --prod",
        "build:dev": "ng build",
        "build:prod": "ng build --prod",
        "start:e2e:mocked": "ng serve --configuration=e2e-mocked",
        "test": "ng test --code-coverage=true --karma-config=karma.parallel.conf.js",
        "test:watch": "ng test --watch=true --karma-config=karma.watch.conf.js",
        "test:linear": "ng test --code-coverage=true --karma-config=karma.conf.js",
        "lint": "npm run lint:eslint && npm run lint:styles",
        "lint:eslint": "eslint --ext .ts webapp/",
        "lint:styles": "stylelint webapp/**/*.scss webapp/**/*.css",
        "lint:fix": "eslint --fix --ext .ts webapp/",
        "lint:staged": "lint-staged",
        "check:dev": "npm run lint && npm run test",
        "check:full": "npm run lint && run-s build:prod test e2e:run:mocked",
        "audit:full": "npm audit --audit-level=low",
        "audit:low": "npm audit --configuration --production --audit-level=low",
        "e2e:install": "cd e2e && npm ci",
        "e2e:open:mocked": "start-server-and-test start:e2e:mocked http-get://localhost:1537/ cypress:open:mocked",
        "e2e:run:mocked": "start-server-and-test start:e2e:mocked http-get://localhost:1537/ cypress:run:mocked",
        "e2e:run:mocked-video": "start-server-and-test start:e2e:mocked http-get://localhost:1537/ cypress:run:mocked",
        "e2e:run:mocked:ci": "npm run e2e:install && npm run e2e:run:mocked",
        "cypress:open:mocked": "cd e2e && npm run cypress:open:mocked",
        "cypress:run:mocked": "cd e2e && npm run cypress:run:mocked",
        "xi18n": "ng xi18n --output-path webapp/locale",
        "ci": "npm ci --ignore-scripts && npm run compile:modules",
        "compile:modules": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
        "preinstall": "npm --ignore-scripts install npm-force-resolutions && npm-force-resolutions && npm run e2e:install",
        "postinstall": "npm run compile:modules",
        "log:versions": "echo NODE version: && node --version && echo NPM version: && npm --version && echo ANGULAR-CLI version: && ng version && echo ESLINT version: && eslint -v && echo STYLELINT version: && stylelint -v",
        "sync:link": "node ./scripts/sync-ngx-design-system-seed/sync.js link",
        "sync:reset": "node ./scripts/sync-ngx-design-system-seed/sync.js reset",
        "sync:reset:reverse": "node ./scripts/sync-ngx-design-system-seed/sync.js reset:reverse",
        "sync:disable": "node ./scripts/sync-ngx-design-system-seed/sync.js disable",
        "sync:status": "node ./scripts/sync-ngx-design-system-seed/sync.js status"
    },
    "private": true,
    "description": "Angular CLI seed",
    "keywords": [
        "angular",
        "material",
        "material design",
        "components",
        "reusable"
    ],
    "license": "Apache-2.0",
    "dependencies": {
        "@angular/animations": "14.2.12",
        "@angular/cdk": "14.2.7",
        "@angular/common": "14.2.12",
        "@angular/compiler": "14.2.12",
        "@angular/core": "14.2.12",
        "@angular/flex-layout": "14.0.0-beta.41",
        "@angular/forms": "14.2.12",
        "@angular/material": "14.2.7",
        "@angular/platform-browser": "14.2.12",
        "@angular/platform-browser-dynamic": "14.2.12",
        "@angular/platform-server": "14.2.12",
        "@angular/router": "14.2.12",
        "detect-libc": "1.0.3",
        "font-awesome": "4.7.0",
        "reset-css": "5.0.1",
        "roboto-fontface": "0.10.0",
        "rxjs": "7.5.7",
        "tslib": "2.3.1",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "14.2.11",
        "@angular/cli": "14.2.10",
        "@angular/compiler-cli": "14.2.12",
        "@angular/language-service": "14.2.12",
        "@ngneat/spectator": "9.0.0",
        "@types/dom-inputevent": "1.0.5",
        "@types/jasmine": "3.6.0",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "14.14.0",
        "@typescript-eslint/eslint-plugin": "5.44.0",
        "@typescript-eslint/parser": "5.44.0",
        "eslint": "8.4.1",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-plugin-cypress": "2.12.1",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jasmine": "4.1.3",
        "husky": "4.3.7",
        "jasmine-core": "3.6.0",
        "jasmine-spec-reporter": "5.0.0",
        "karma": "6.3.17",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage": "2.0.3",
        "karma-jasmine": "4.0.0",
        "karma-jasmine-html-reporter": "1.5.0",
        "karma-parallel": "0.3.1",
        "karma-spec-reporter": "0.0.32",
        "lint-staged": "10.5.0",
        "ng-lint-staged": "0.1.6",
        "ng-mocks": "14.3.3",
        "npm-force-resolutions": "^0.0.10",
        "npm-run-all": "4.1.5",
        "replace-in-file": "6.3.2",
        "start-server-and-test": "1.11.7",
        "stylelint": "13.7.2",
        "stylelint-config-standard": "20.0.0",
        "ts-node": "9.0.0",
        "typescript": "4.8.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": "eslint",
        "*.css": "stylelint",
        "*.scss": "stylelint --syntax=scss"
    },
    "engines": {
        "node": ">=16.14.0",
        "npm": ">=8.3.1"
    },
    "resolutions": {}
}
