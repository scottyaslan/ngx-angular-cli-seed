{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "app": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "webapp",
            "prefix": "",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "allowedCommonJsDependencies": [
                        ],
                        "polyfills": "webapp/polyfills.ts",
                        "preserveSymlinks": true,
                        "aot": false,
                        "assets": [
                            "webapp/favicon.ico",
                            "webapp/assets",
                            "webapp/platform/assets"
                        ],
                        "styles": [
                            "node_modules/reset-css/sass/_reset.scss",
                            "node_modules/font-awesome/css/font-awesome.min.css",
                            "webapp/platform/assets/material/theme.scss",
                            "webapp/assets/styles.scss"
                        ],
                        "scripts": [],
                        "baseHref": "",
                        "outputPath": "dist/ngx-angular-cli-seed",
                        "index": "webapp/index.html",
                        "main": "webapp/main.ts",
                        "tsConfig": "tsconfig.webapp.json",
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "webapp/environments/environment.ts",
                                    "with": "webapp/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumError": "6mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "outputPath": "dist/ngx-angular-cli-seed",
                            "index": "webapp/index.html",
                            "main": "webapp/main.ts",
                            "tsConfig": "tsconfig.webapp.json"
                        },
                        "mocked": {
                            "fileReplacements": [
                                {
                                    "replace": "webapp/environments/environment.ts",
                                    "with": "webapp/environments/environment.mocked.ts"
                                }
                            ],
                            "outputPath": "dist/ngx-angular-cli-seed",
                            "index": "webapp/index.html",
                            "main": "webapp/main.ts",
                            "tsConfig": "tsconfig.webapp.json"
                        },
                        "e2e-mocked": {
                            "fileReplacements": [
                                {
                                    "replace": "webapp/environments/environment.ts",
                                    "with": "webapp/environments/environment.e2e.mocked.ts"
                                }
                            ],
                            "outputPath": "dist/ngx-angular-cli-seed",
                            "index": "webapp/index.html",
                            "main": "webapp/main.ts",
                            "tsConfig": "tsconfig.webapp.json"
                        },
                        "defaultConfiguration": ""
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "app:build",
                        "port": 1337
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "app:build:production"
                        },
                        "mocked": {
                            "browserTarget": "app:build:mocked",
                            "port": 1437
                        },
                        "e2e-mocked": {
                            "browserTarget": "app:build:e2e-mocked",
                            "port": 1537
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "app:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "webapp/test.ts",
                        "codeCoverageExclude": [
                            "webapp/testing/mock-interceptor/**"
                        ],
                        "polyfills": "webapp/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "webapp/favicon.ico",
                            "webapp/assets"
                        ],
                        "styles": [],
                        "scripts": [],
                        "fileReplacements": [
                            {
                                "replace": "webapp/environments/environment.ts",
                                "with": "webapp/environments/environment.test.ts"
                            }
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.webapp.json",
                            "tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "app",
    "cli": {
        "analytics": false
    }
}
